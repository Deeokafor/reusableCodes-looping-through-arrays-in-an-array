// Write your code below
let bobsFollowers = ['tim', 'bob', 'julia', 'nedu'];
let tinasFollowers = ['tim', 'bob', 'holt'];

let mutualFollowers = [];
let unmutualFollowers = [];

for(let c = 0; c < bobsFollowers.length; c++){
  for(let t = 0; t < tinasFollowers.length; t++){
    if(bobsFollowers[c] === tinasFollowers[t]){
   		mutualFollowers.push(tinasFollowers[t]);
      //unmutualFollowers.push(bobsFollowers[c]);
       }
  }
}

console.log(mutualFollowers);
//console.log(unmutualFollowers);
